- var zero = result.length == 0

doctype html
html
    head
        include partials/head
    body
       .container-fluid
            include partials/navbar
           .row.clearfix
               .col-xs-3#page-selector-menu
                   if zero
                       ul.nav.nav-tabs.tabs-left
                           li.active
                               a(data-toggle='tab', href='#solun') solun
                           li
                               a(data-toggle='tab', href='#ellinair') ellinair

               .col-xs-9
                   .row.clearfix
                       .btn-group
                            button.btn.btn-primary Graph
                            button.btn.btn-primary Numbers


                   .tab-content.top-buffer
                       .tab-pane.active#solun
                           p solun page
                       .tab-pane#ellinair
                           p ellinair page #{length}


    include partials/footer